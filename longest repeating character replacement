class Solution(object):
    def characterReplacement(self, s, k):
        l,temp,res=0,0,0
        dic={}
        for r in range(len(s)):
            dic[s[r]]=dic.get(s[r],0)+1
            temp=max(temp,dic[s[r]])
            while((r-l)+1)-temp>k:
                dic[s[l]]-=1
                l+=1
            res=max(res,(r-l)+1)
        return res
